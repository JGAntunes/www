{
  "name": "ampersand",
  "author": "Francisco Dias",
  "version": "0.0.0",
  "main": "server.js",
  "scripts": {
    "test": "standard",
    "start": "NODE_PATH=. node server.js",
    "mon": "NODE_PATH=. nodemon server.js --ignore client/"
  },
  "dependencies": {
    "ampersand-dom": "^1.2.1",
    "ampersand-input-view": "^4.0.1",
    "ampersand-rest-collection": "^2.0.0",
    "ampersand-router": "^1.0.1",
    "ampersand-view": "^7.0.1",
    "ampersand-view-switcher": "^2.0.0",
    "bows": "^1.3.2",
    "clientconfig": "^1.0.0",
    "domify": "^1.3.0",
    "domready": "^1.0.5",
    "favicon-setter": "^0.1.1",
    "hapi": "^8.2.0",
    "jquery": "^2.1.3",
    "moonboots_hapi": "^4.0.2",
    "stylizer": "^2.0.0",
    "templatizer": "^1.0.0",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.3.7",
    "standard": "^2.10.0"
  },
  "jshintConfig": {
    "asi": false,
    "browser": true,
    "curly": false,
    "expr": true,
    "indent": 4,
    "loopfunc": true,
    "node": true,
    "trailing": true,
    "undef": true,
    "white": true
  },
  "standard": {
    "ignore": [
      "**/vendor/**",
      "client/js/templates.js"
    ]
  }
}
